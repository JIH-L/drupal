{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "require": {
        "ckeditor-plugin/colorbutton": "^4.11",
        "ckeditor-plugin/panelbutton": "^4.11",
        "ckeditor/resize": "^4.13",
        "ckeditor/youtube": "^2.1",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "~1.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/amswap": "^3.0",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_visibility_groups": "^1.4",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/ckeditor_font": "^1.2",
        "drupal/colorbutton": "^1.2",
        "drupal/config_split": "^1.7",
        "drupal/console": "~1.0",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/custom_entity_pager": "^1.0@alpha",
        "drupal/custom_view_filters": "^2.0",
        "drupal/devel": "^4.1",
        "drupal/eck": "^1.0@beta",
        "drupal/entity_pager": "^1.0@RC",
        "drupal/entity_type_clone": "^1.7",
        "drupal/entityqueue": "^1.2",
        "drupal/environment_indicator": "^4.0",
        "drupal/fontawesome": "^2.19",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_lb": "^1.0@beta",
        "drupal/gin_login": "^1.0@RC",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/gutenberg": "^2.0",
        "drupal/image_widget_crop": "^2.3",
        "drupal/insert_view_adv": "^2.0@alpha",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/jquery_ui_touch_punch": "1.0.x-dev@dev",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/pager": "^1.4",
        "drupal/pagerer": "^2.1",
        "drupal/panelbutton": "^1.3",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/photoswipe": "^3.0@alpha",
        "drupal/prevnext": "^2.0",
        "drupal/publication_date": "^2.0@beta",
        "drupal/restui": "^1.20",
        "drupal/selective_better_exposed_filters": "^2.0@beta",
        "drupal/stage_file_proxy": "^1.1",
        "drupal/swiftmailer": "^2.0",
        "drupal/taxonomy_menu": "3.x-dev@dev",
        "drupal/tvi": "^1.0@RC",
        "drupal/twig_tweak": "^3.1",
        "drupal/twig_xdebug": "^1.2",
        "drupal/twigsuggest": "1.x-dev@dev",
        "drupal/webform": "^6.0",
        "drupal/weight": "^3.3",
        "drupal/workbench": "^1.3",
        "furf/jquery-ui-touch-punch": "dev-master",
        "npm-asset/photoswipe": "^4.1",
        "oomphinc/composer-installers-extender": "~2.0"
    },
    "require-dev": {
        "drush/drush": "^10.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/resize",
                "version": "4.13.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/resize/releases/resize_4.13.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "version": "4.14.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.14.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/youtube",
                "version": "2.1.14",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/youtube/releases/youtube_2.1.14.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/panelbutton",
                "version": "4.11.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.11.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/colorbutton",
                "version": "4.11.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.11.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false,
                "[web-root]/web.config": false,
                "[web-root]/sites/development.services.yml": false
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "drupal-libraries": {
            "library-directory": "web/libraries",
            "libraries": []
        },
        "patches": {
            "drupal/bootstrap_barrio": {
                "Conflict of library names in theme and core.": "https://www.drupal.org/files/issues/2021-01-13/3192490-2.patch"
            },
            "drupal/svg_image": {
                "Query condition '%s %s ()' cannot be empty.": "https://www.drupal.org/files/issues/2020-06-25/empty_field_fix-3154892-2.patch"
            },
            "drupal/publication_date": {
                "Module is using deprecated FieldFormatter \"hidden\" that causing configuration import issues in Drupal9": "https://www.drupal.org/files/issues/2020-09-25/hidden_plugin_doesnt_exist_3173230-2.patch"
            },
            "drupal/amswap": {
                "Function #pre_render callbacks are deprecated": "https://www.drupal.org/files/issues/2020-08-13/amswap-pre_render_callbacks_deprecated-3149894-4.patch"
            },
            "drupal/simple_menu_permissions": {
                "Can not add links to the menu due to missing menu entity parameter.": "https://www.drupal.org/files/issues/2021-03-18/simple_menu_permissions-8.x-1.3-no-path-logic.patch"
            }
        },
        "patches-ignore": {}
    }
}